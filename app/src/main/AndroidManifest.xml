<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ao.countdown">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/relax"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity" android:theme="@style/AppTheme.Full">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main"></activity>
        <activity android:name=".OceanActivity"></activity>
        <activity android:name=".FireActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <service android:name=".AudioService"
            android:exported="true"
            android:process=":remote">
        </service>
        <service android:name=".AudioService2"
            android:exported="true"
            android:process=":remote">
        </service>
        <service android:name=".AudioService3"
            android:exported="true"
            android:process=":remote">
        </service>
        <receiver android:name=".Notification" android:process=":remote"></receiver>
    </application>


</manifest>